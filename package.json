  "description": "Model Context Protocol server that manages persistent context memories for integration with VS Code",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "context-memory-mcp-server": "dist/index.js"
  },
  "scripts": {
